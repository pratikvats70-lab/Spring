<?xml version="1.0" encoding="UTF-8"?>
<wsdl:definitions xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
                  xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
                  xmlns:tns="http://example.com/customer"
                  xmlns:xs="http://www.w3.org/2001/XMLSchema"
                  targetNamespace="http://example.com/customer">

    <wsdl:types>
        <xs:schema>
            <xs:import namespace="http://example.com/customer"
                      schemaLocation="../xsd/customer.xsd"/>
        </xs:schema>
    </wsdl:types>

    <wsdl:message name="customerRequest">
        <wsdl:part name="customer" element="tns:customer"/>
    </wsdl:message>

    <wsdl:message name="customerResponse">
        <wsdl:part name="customer" element="tns:customer"/>
    </wsdl:message>

</wsdl:definitions>
