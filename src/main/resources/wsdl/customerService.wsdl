<?xml version="1.0" encoding="UTF-8"?>
<wsdl:definitions xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
                  xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
                  xmlns:tns="http://example.com/customer"
                  xmlns:xs="http://www.w3.org/2001/XMLSchema"
                  targetNamespace="http://example.com/customer">

    <wsdl:types>
        <xs:schema>
            <xs:import namespace="http://example.com/customer"
                      schemaLocation="../xsd/customer.xsd"/>
        </xs:schema>
    </wsdl:types>

    <wsdl:message name="customerRequest">
        <wsdl:part name="customer" element="tns:customer"/>
    </wsdl:message>

    <wsdl:message name="customerResponse">
        <wsdl:part name="customer" element="tns:customer"/>
    </wsdl:message>

    <wsdl:portType name="CustomerService">
        <wsdl:operation name="processCustomer">
            <wsdl:input message="tns:customerRequest"/>
            <wsdl:output message="tns:customerResponse"/>
        </wsdl:operation>
    </wsdl:portType>

    <wsdl:binding name="CustomerServiceSoap11" type="tns:CustomerService">
        <soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>
        <wsdl:operation name="processCustomer">
            <soap:operation soapAction=""/>
            <wsdl:input>
                <soap:body use="literal"/>
            </wsdl:input>
            <wsdl:output>
                <soap:body use="literal"/>
            </wsdl:output>
        </wsdl:operation>
    </wsdl:binding>

    <wsdl:service name="CustomerServiceImpl">
        <wsdl:port name="CustomerServiceSoap11" binding="tns:CustomerServiceSoap11">
            <soap:address location="http://example.com/customer"/>
        </wsdl:port>
    </wsdl:service>

</wsdl:definitions>
